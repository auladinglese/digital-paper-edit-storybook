{
  "name": "@bbc/digital-paper-edit-react-components",
  "version": "1.3.0",
  "description": "React Component library for DPE",
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "BABEL_ENV=production babel src -d dist",
    "prepare": "npm run build",
    "test": "react-scripts test",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "build-storybook-docs": "build-storybook -s public --docs",
    "start": "npm run storybook",
    "deploy:ghpages": "npm run build-storybook && gh-pages -d public",
    "publish:public": "npm run build:component && rm ./dist/package.json || true && cp package.json ./dist/package.json && rm ./dist/README.md || true && cp README.md ./dist/README.md || true && npm publish dist --access public",
    "publish:dry:run": "npm publish --dry-run",
    "lint:fix": "eslint ./packages/* --fix",
    "release": "auto shipit"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "author": "Eimi Okuno <eimi.okuno@bbc.co.uk>",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.5.5",
    "@bbc/react-transcript-editor": "^1.0.4",
    "@fortawesome/fontawesome-svg-core": "^1.2.19",
    "@fortawesome/free-brands-svg-icons": "^5.8.2",
    "@fortawesome/free-solid-svg-icons": "^5.10.1",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "array-move": "^2.1.0",
    "auto": "^7.4.3",
    "bootstrap": "^4.3.1",
    "bootstrap-css-only": "^4.3.1",
    "chroma-js": "^2.0.3",
    "core-js": "^3.3.4",
    "css-color-names": "^1.0.1",
    "cuid": "^2.1.6",
    "downloadjs": "^1.4.7",
    "edl_composer": "^1.0.3",
    "express": "^4.17.1",
    "gel-typography": "^2.0.5",
    "polished": "^3.4.1",
    "prettier": "^1.18.2",
    "prop-types": "^15.7.2",
    "randomcolor": "^0.5.4",
    "react-bootstrap": "^1.0.0-beta.11",
    "react-color": "^2.17.1",
    "react": "^16.10.4",
    "react-dom": "^16.12.0",
    "react-router": "^5.1.2",
    "react-confirm": "^0.1.18",
    "react-scripts": "^3.2.0",
    "react-select": "^2.4.3",
    "react-sortable-hoc": "^1.10.1",
    "save": "^2.4.0",
    "styled-components": "^4.3.2",
    "videocontext": "^0.53.0"
  },
  "peerDependencies": {
    "react": "^16.10.4",
    "react-dom": "^16.12.0",
    "react-router": "^5.1.2"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^5.2.1",
    "@storybook/addon-actions": "^5.2.1",
    "@storybook/addon-docs": "^5.2.1",
    "@storybook/addon-knobs": "^5.2.1",
    "@storybook/addon-links": "^5.2.1",
    "@storybook/addon-storysource": "^5.2.1",
    "@storybook/addon-viewport": "^5.0.8",
    "@storybook/addons": "^5.2.1",
    "@storybook/react": "^5.2.1",
    "auto": "^7.4.3",
    "gh-pages": "^2.0.1",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "storybook-react-router": "^1.0.5"
  },
  "repository": "https://github.com/bbc/digital-paper-edit-storybook.git",
  "private": false
}
